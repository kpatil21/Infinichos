<div class="admin-title">
    <h2>All Blogs.</h2>
    <a href="/add-post" class="button">+ Add New Blog</a>
</div>

<ul class="admin-posts">
    <% data.forEach(post => { %>
     
       <li>
        <a href="/post/<%= post._id %>"><%= post.title %> &nearr;</a>

        <div class="admin-post-controls">
            <a href="/edit-post/<%= post._id %>" class="btn">Update</a>

            <form action="/delete-post/<%= post._id %>?_method=DELETE" method="POST">
                <input type="submit" value="Delete" class="btn-delete">
            </form>
        </div>
       </li>

    <% }) %>
</ul>